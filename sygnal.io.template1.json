{
    "providerId": "sygnal.io",
    "providerName": "Sygnal",
    "serviceId": "template1",
    "serviceName": "Sygnal Tracking Subdomain",
    "version": 1,
    "description": "Service to create a subdomain for tracking purposes.",
    "syncRedirectDomain": "sygnal.io",
    "syncPubKeyDomain": "sygnal.io",
    "syncBlock" : false,
    "variableDescription": "host is the subdomain you want to use for tracking",
    "records": [
        {
            "type": "CNAME",
            "host": "%host%",
            "pointsTo": "app.securetrack.io",
            "ttl": 3600
        }
    ]
}
